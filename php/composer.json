{
    "name": "svetogor/assistant",
    "type": "project",
    "require": {
        "laudis/neo4j-php-client": "dev-main",
        "slim/slim": "^4.10",
        "slim/psr7": "dev-master",
        "php-di/php-di": "^7.0",
        "php-di/slim-bridge": "^3.4",
        "aura/session": "2.x-dev",
        "nikic/php-parser": "^5.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "maximebf/debugbar": "1.23.x-dev",
        "symfony/var-dumper": "7.2.x-dev",
        "symfony/web-profiler-bundle": "7.2.x-dev",
        "barryvdh/laravel-debugbar": "3.14.x-dev",
        "middlewares/debugbar": "dev-master",
        "middlewares/whoops": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "svetogor"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
